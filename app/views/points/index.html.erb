<p id="notice"><%= notice %></p>

<h1>Points</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Phoneone</th>
      <th>Phonetwo</th>
      <th>Emailone</th>
      <th>Emailtwo</th>
      <th>Name</th>
      <th>Address</th>
      <th>Worktime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @points.each do |point| %>
      <tr>
        <td><%= point.title %></td>
        <td><%= point.phoneone %></td>
        <td><%= point.phonetwo %></td>
        <td><%= point.emailone %></td>
        <td><%= point.emailtwo %></td>
        <td><%= point.name %></td>
        <td><%= point.address %></td>
        <td><%= point.worktime %></td>
        <td><%= link_to 'Show', point %></td>
        <td><%= link_to 'Edit', edit_point_path(point) %></td>
        <td><%= link_to 'Destroy', point, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Point', new_point_path %>
