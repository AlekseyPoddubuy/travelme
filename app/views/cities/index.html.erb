<p id="notice"><%= notice %></p>

<h1>Cities</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Phoneone</th>
      <th>Phonetwo</th>
      <th>Phonethree</th>
      <th>Emailone</th>
      <th>Emailtwo</th>
      <th>Worktime</th>
      <th>Adress</th>
      <th>Name</th>
      <th>Surname</th>
      <th>Facebook</th>
      <th>Instagram</th>
      <th>Slug</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cities.each do |city| %>
      <tr>
        <td><%= city.title %></td>
        <td><%= city.phoneone %></td>
        <td><%= city.phonetwo %></td>
        <td><%= city.phonethree %></td>
        <td><%= city.emailone %></td>
        <td><%= city.emailtwo %></td>
        <td><%= city.worktime %></td>
        <td><%= city.adress %></td>
        <td><%= city.name %></td>
        <td><%= city.surname %></td>
        <td><%= city.facebook %></td>
        <td><%= city.instagram %></td>
        <td><%= city.slug %></td>
        <td><%= link_to 'Show', city %></td>
        <td><%= link_to 'Edit', edit_city_path(city) %></td>
        <td><%= link_to 'Destroy', city, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New City', new_city_path %>
